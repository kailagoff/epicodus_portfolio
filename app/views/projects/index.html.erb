<% @projects.each do |project| %>
  <div class="col-md-4">
    <div id="projects_list">
      <h6><%= link_to project.project_name, project_path(project), id: "#{project.id}", remote: true %></h6>
      <div id="project_repo_<%= project.id %>"></div>
      <h6><%= link_to "Github Repo" , project.project_link, :target => "_blank" %></h6>
      <%= link_to image_tag(project.image.url(:large)), project_path(project) %>
    </div>
  </div>
<% end %>
