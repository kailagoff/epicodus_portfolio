<% @projects.each do |project| %>
  <div class="col-md-4">
    <div id="projects_list">
      <h6><%= link_to project.project_name, project_path(project), remote: true %></h6>
      <h6 id="project_repo"><%= link_to project.project_link %></h6>
      <%= link_to image_tag(project.image.url(:large)), project_path(project) %>
    </div>
  </div>
<% end %>
