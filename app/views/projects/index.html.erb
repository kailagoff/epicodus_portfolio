

<% @projects.each do |project| %>
  <div class="col-md-4">
    <div id="projects_list">
      <h6><%= link_to project.project_name, project_path(project) %></h6>
      <%= link_to image_tag(project.image.url(:thumb)), project_path(project) %>
    </div>
  </div>
<% end %>



<% if current_user %>
  <p><%= link_to "New project", new_project_path, class: "btn btn-info" %></p>
<% end %>
