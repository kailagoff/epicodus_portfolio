<h1><%= @project.project_name %></h1>
<h6><%= link_to "Github Repo" , @project.project_link, :target => "_blank" %></h6>
<h1><%= image_tag @project.image.url(:thumb) %></h1>

<p><%= link_to "Return to projects", projects_path, class: "btn btn-info" %></p>
<p><%= link_to "Edit project", edit_project_path, class: "btn btn-info" %></p>
<p><%= link_to "Delete post", project_path(@project), :data => {:confirm => "Are you sure you want to delete this project?", :method => "delete"}, class: "btn btn-info" %></p>
