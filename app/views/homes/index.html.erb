<header>
  <div class="container" align="center">
      <!-- Modal -->
      <div class="modal fade" id="myModal" role="dialog">
         <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
               <div class="modal-header">
                  <button type="button" class="close" data-dismiss="modal">&times;</button>
                  <h4 class="modal-title">Modal Header</h4>
               </div>
               <div class="modal-body">
                  <p>Some text in the modal.</p>
               </div>
               <div class="modal-footer">
                  <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
               </div>
            </div>
         </div>
      </div>
   </div>


  <div class="title">
    <nav class="navbar navbar-transparent" data-spy="affix" data-offset-top="197">
      <div class="nav navbar-nav">
        <li class="active"></li>
        <li><button type="button" class="modalButton" data-name="Clicked Modal 1" data-toggle="modal">about</button></li>
        <li><button type="button" class="modalButton" data-name="Clicked Modal 2" data-toggle="modal">skills</button></li>
        <li><button type="button" class="modalButton" data-name="Clicked Modal 3" data-toggle="modal" >projects</button></li>
        <li><button type="button" class="modalButton" data-name="Clicked Modal 4" data-toggle="modal">contact</button></li>
      <div/>
    </nav>




    <div class="row" id="header-row">
      <br><br>
      <div>
        <%= image_tag("me.jpg", :class=> "img-circular") %>
      </div>
      <br>
      <div class="column right">
        <h2>KAILA GOFF</h2>
        <h5>SOFTWARE DEVELOPER</h5>
        <h6>portland, or</h6>
      </div>
    </div>
    <div id="footer">
      <%= image_tag("kgicondraft.png", :size => "100x67") %>
    </div>
  </div>
</header>
